<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate
            android:fromDegrees="90"
            android:toDegrees="90"
            android:pivotX="50%"
            android:pivotY="50%">
            <shape android:shape="line">
                <stroke
                    android:color="@color/darkGreyColor"
                    android:dashWidth="2sp"
                    android:dashGap="1dp"
                    android:width="1dp" />
            </shape>
        </rotate>
    </item>
</layer-list>

